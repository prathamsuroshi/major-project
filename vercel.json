{
    "version":2,
    "builds": [
        {
            "src": "package.json",
            "use": "vercel-sapper"
        },
        {
            "src": "./index.py",
            "use": "@vercel/python",
            "config":{"runtime":"python3.10.2"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/",
            "headers": { 
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}



